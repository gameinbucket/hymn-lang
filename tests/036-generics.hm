type vec
	x float
	y float
	z float

type link<t>
	node t
	next maybe<link<t>>

type array<d>
	data []d
	size int

type attribute<v>
	on  bool
	has v

type node<t, w>
	data  t
	make  w
	value int
	use   attribute<string>
	nest  attribute<t>

main
	e = link<vec>()
	e.node = vec()
	e.node.z = 122.4
	e.next = link()
	e.next.node = vec()
	e.next.node.y = 144.3
	echo(e.node.z)
	echo(e.next.node.y)
