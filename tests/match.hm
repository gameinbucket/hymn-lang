enum bug
  ant(int)
  bee
  moth

goob(b bug) string
  match b
    ant => return "glob ant!"
    bee => return "glob bee!"
    moth => return "glob moth!"
  
doob(d dog) string
  match d
    bark => return "bark!"
    woof => echo("ha!")
  return "woof!"

zoob(b bug.ant) string
  return "zoob!"

