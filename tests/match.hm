enum bug
  ant(int)
  bee
  moth

main
  x = bug.ant(3)
  
  match x
    ant
      echo(x.0)
    else pass
  
  match x ant: echo(x.0)
    
  match x ant
    echo(x.0)

  match x
    ant
      echo(x.0)
      echo("ant!")
    bee
      echo("bee!")
    moth
      echo("moth!")

  match x
    ant: echo("ant!")
    bee: echo("bee!")
    moth: echo("moth!")

  match x
    bee
      echo("bee!")
    else
      echo("other!")
