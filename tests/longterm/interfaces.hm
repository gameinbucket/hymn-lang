interface expl
  get() int
  set(value int)

type foo
  implement expl
  x int

foo.get()
  return self.x

foo.set(value int)
  self.x = value

type bar
  implement expl
  y int
  
bar.get()
  return self.y

bar.set(value int)
  self.y = value

type special
  one expl
  two expl

main
  a = foo(x:2)
  echo(a.x)

  b = bar(y:3)
  echo(b.y)

  e = special
  e.one = a
  e.two = b

  e.one.set(4)
  e.two.set(5)

  echo(e.one.get())
  echo(e.two.get())
