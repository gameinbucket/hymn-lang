type foo<k, v>
	size     k
	capacity k
	name     v
	list     []k
	im_list  [3]v
	mutate   (v) v
	total    int
	weight   char

set_foo(f foo<int, string>, one int, two string)
	f.size = one
	f.name = two

transform(in string) string
	return "transform_" + in

foo.set(one k, two v)
	self.list[0] = one
	self.im_list[0] = two

foo.get_size() k
	return self.size

foo.get_total() int
	return self.total

main
	g = foo(
		size:22.33
		name:"zoob"
		mutate:transform)
	echo(g.size)
	echo(g.name)
	g.im_list[1] = "number one"
	echo(g.im_list[1])
	echo("---")

	f = foo(size:11, name:"goob", mutate:transform)
	echo(f.size)
	echo(f.name)
	f.im_list[2] = "number 2"
	echo(f.im_list[2])
	echo("---")

	set_foo(f, 22, "foobar")
	echo(f.size)
	echo(f.name)
	echo("---")

	(* f.set(22, "foobar")
	echo(f.get_size())
	echo(f.get_total()) *)
	echo("---")
	
