enum operation
    add
    subtract
    multiply
    divide

def get_op(flag bool) maybe<operation>
    if flag
        return operation.multiply
    return none

def main()
    x = get_op(true)
    y = get_op(false)
    match x
        none => echo("oh no!")
        some(s) => echo("x :=", s)
    if y is some: echo("oh no!")
