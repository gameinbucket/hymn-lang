enum operation
    add
    subtract
    multiply
    divide

class ptr<x>
    val x

def call(flag bool) maybe<ptr<operation>>
    if flag
        return ptr(operation.multiply)
    return none

def main()
    x = call(true)
    y = call(false)
    match x
        none => echo("oh no!")
        some(s) =>
            match s.val
                multiply => echo("x := multiply")
                _ => echo("oh no!")
    if y is some: echo("oh no!")
