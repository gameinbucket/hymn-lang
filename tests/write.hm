main
	mutable pwd = system("pwd")
	(* if pwd[len(pwd) - 1] == '\n' *)
	pwd = substring(pwd, 0, len(pwd) - 1)
	mutable date = system("date +%s")
	size = len(date)
	(* if date[size - 1] == '\n' *)
	date = substring(date, 0, size - 1)
	path = pwd + "/out/" + date + ".txt"
	content = "hello file\n"
	echo("path:", path)
	echo("content:", content)
	write(path, content)
	echo("done!")
