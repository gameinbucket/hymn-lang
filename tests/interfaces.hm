
interface get_and_set
    get () int
    set (int)

interface read_and_write
    read() uint8
    write(uint8)

class foo: get_and_set
    one int

class bar: get_and_set, read_and_write
    two   int
    three uint8

foo get() int
    return self.one

foo set(value int)
    self.one = value

bar get() int
    return self.two

bar set(value int)
    self.two = value

bar read() uint8
    return self.three + uint8 1

bar write(value uint8)
    self.three += value

main
    f = foo(2)
    f.set(3)
    echo(f.get())

    b = bar(4, uint8 5)
    b.set(6)
    echo(b.get())
    b.write(uint8 7)
    echo(b.read())
