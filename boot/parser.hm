import stream
import tokenizer
import parsing_errors (parse_error)

class parser
    out  string
    path string
    pos  uint
    line uint

def parse(out string, path string)
    echo(out)
    echo(path)
    data = cat(path)
    file_stream = stream.new_stream(data)
    tokens = tokenizer.new_tokenizer(file_stream)
    token = tokens.get(0)
    echo(data)
    echo(token.to_string())

def parser.eat(value string) maybe<parse_error>
    self.verify(value)
    self.next()
