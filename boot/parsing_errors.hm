import parser (parser)

class parse_error
    code        uint
    description string
    line        uint

def new(p parser, code uint, description string) parse_error
    return parse_error (
        code: code
        description: description
        line: p.line
    )

def parse_error.to_string() string
    mutable out = ""
    out += sprintf("Code: %04d", self.code)
    return out
