class parse_error
    line        int
    file        string
    name        string
    description string
    hint        string

def parse_error.to_string() string
    mutable out = "\n"
    out += "-- " + self.name + " -- " + self.file
    out += "\n\n" + self.description
    out += "\n\n" + self.hint
    out += "\n\n"
    return out
