import function (hm_function)
import class (hm_class)

import std:hashmap (
    hashmap
    new_str_map
)

class hm_lib
    functions     hashmap<string, hm_function>
    function_list []hm_function
    types         hashmap<string, string>
    classes       hashmap<string, hm_class>

def new_hm_lib() hm_lib
    return hm_lib (
        functions: new_str_map<hm_function>()
        function_list: _
        types: new_str_map<string>()
        classes: new_str_map<hm_class>()
    )
