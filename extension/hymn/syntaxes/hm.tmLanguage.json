{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "hymn",
	"scopeName": "source.hymn",
	"patterns": [{
		"explanation": "comments",
		"begin": "\\(\\*",
		"end": "\\*\\)",
		"name": "comment.block.hymn"
	}, {
		"explanation": "quotes",
		"match": "\".*?\"",
		"name": "string.quoted.double.hymn",
		"patterns": [{
			"match": "\\\\.",
			"name": "constant.character.escape.hymn"
		}]
	}, {
		"explanation": "self",
		"match": "\\bself\\b",
		"name": "variable.language.this.hymn"
	}, {
		"explanation": "types",
		"match": "^(type|enum) +([a-z_0-9]+)",
		"captures": {
			"1": {
				"name": "keyword.struct.hymn"
			},
			"2": {
				"name": "entity.name.type.hymn"
			}
		}
	}, {
		"explanation": "modifiers",
		"match": "\\b(mutable|immutable)\\b",
		"name": "storage.modifier.hymn"
	}, {
		"explanation": "booleans",
		"match": "\\b(true|false|null)\\b",
		"name": "constant.language.hymn"
	}, {
		"explanation": "optionals",
		"match": "[^a-zA-Z0-9_](maybe|some|none)[^a-zA-Z0-9_]",
		"captures": {
			"1": {
				"name": "entity.name.type.hymn"
			}
		}
	}, {
		"explanation": "numerics",
		"match": "[^a-zA-Z0-9_](int|int8|int16|int32|int64|float32|float64|float|uint8|uint16|uint32|uint64|byte|char)[^a-zA-Z0-9_]",
		"captures": {
			"1": {
				"name": "storage.type.numeric.hymn"
			}
		}
	}, {
		"explanation": "boolean",
		"match": "[^a-zA-Z0-9_](bool)[^a-zA-Z0-9_]",
		"captures": {
			"1": {
				"name": "storage.type.boolean.hymn"
			}
		}
	}, {
		"explanation": "string",
		"match": "[^a-zA-Z0-9_](string)[^a-zA-Z0-9_]",
		"captures": {
			"1": {
				"name": "storage.type.string.hymn"
			}
		}
	}, {
		"explanation": "flow",
		"match": "\\b(if|elif|else|for|return|break|continue|and|or|not|match|is|pass|goto|label)\\b",
		"name": "keyword.control.hymn"
	}, {
		"explanation": "class function",
		"match": "^([a-z_0-9]+)\\.([a-z_0-9]+)($|\\(| +)",
		"captures": {
			"1": {
				"name": "entity.name.function.hymn"
			},
			"2": {
				"name": "entity.name.function.hymn"
			}
		}
	}, {
		"explanation": "file function",
		"match": "^([a-z_0-9]+)($|\\(| +)",
		"captures": {
			"1": {
				"name": "entity.name.function.hymn"
			}
		}
	}, {
		"explanation": "file function call",
		"match": "([a-z_0-9]+)\\(",
		"captures": {
			"1": {
				"name": "support.function.hymn"
			}
		}
	}, {
		"explanation": "call",
		"match": "\\.([a-z_0-9]+)\\(",
		"captures": {
			"1": {
				"name": "support.function.hymn"
			}
		}
	}, {
		"explanation": "assignment",
		"match": "^ +([a-z_0-9]+(?:\\.[a-z_0-9]+)*)(?: +)?(?:=|\\+=|-=|\/=|\\*=|\\^=|\\|=|&=|<<=|>>=)",
		"captures": {
			"1": {
				"name": "variable.other.assignment.hymn"
			}
		}
	}, {
		"explanation": "numbers",
		"match": "\\b[0-9]+(.[0-9]+)?\\b",
		"name": "constant.numeric.decimal.hymn"
	}, {
		"explanation": "comparison",
		"match": "(if|for|=).*? (>=|<=|!=|<|>|=) ",
		"captures": {
			"2": {
				"name": "keyword.operator.comparison.hymn"
			}
		}
	}, {
		"explanation": "alias",
		"match": "^(alias) +([a-z_0-9]+) +(->) +([a-z_0-9]+)$",
		"captures": {
			"1": {
				"name": "keyword.struct.hymn"
			},
			"2": {
				"name": "entity.name.type.hymn"
			},
			"3": {
				"name": "support.function.hymn"
			},
			"4": {
				"name": "entity.name.type.hymn"
			}
		}
	}]
}
