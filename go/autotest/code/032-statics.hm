type foo
  x string

immutable name = "foo"
immutable foo_pool = [3]foo
immutable foo_single = foo()

mutable next_id = 0

do_update(z int)
  next_id += z

main
  next_id += 1
  echo(next_id)
  do_update(2)
  echo(next_id)
  echo(name)
  foo_pool[0] = foo()
  foo_pool[0].x = "yo mama"
  foo_single.x = "big boi"
  echo(foo_pool[0].x)
  echo(foo_single.x)
