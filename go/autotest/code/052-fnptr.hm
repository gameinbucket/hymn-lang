type typ
  ptr(int) int

fun(ptr() int)
  echo(ptr())
  
get(x int) int
  return x

get2(x int) int
  return x * x

get3() int
  return 4

foo()
  echo("foo")

main
  a = get
  echo(a(3))

  mutable b = get
  b = get2
  echo(b(3))

  c = get3
  echo(c())

  f = foo
  f()

  t = typ(get)
  echo(t.ptr(3))

  fun(get3)
