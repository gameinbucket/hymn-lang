class vec
    x float
    y float
    z float

enum operation
    add
    subtract
    multiply
    divide

const pi = 3.1415

def power(x int) int
    return x * x

def vec.dot() float
    return self.x * self.y * self.z

def vec.init(x float, y float, z float) vec
    self.x = x
    self.y = y
    self.z = z
    return self
