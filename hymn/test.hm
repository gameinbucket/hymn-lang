class unit_test
    file_name     string
    function_name string
    result        string
    failed        bool

class test
    # test_list []unit_test
    test_list []string
    messages  []string
    failed    bool

def test.assert(expected string, actual string) bool
    if expected == actual
        return true
    self.error("Expected `" + expected + "` but found `" + actual + "`")
    return false

def test.assert_true(actual bool) bool
    if actual
        return true
    self.error("Expected `true` but found `false`")
    return false

def test.error(message string)
    self.messages = push(self.messages, message)
    self.failed = true

